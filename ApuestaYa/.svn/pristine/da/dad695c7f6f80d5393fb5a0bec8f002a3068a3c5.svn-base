package model;

public class ApuestaSencilla extends ApuestaRealizada {
	// Para obtener los datos de la apuesta sobre la que realizamos el
	// pronostico
	private String apuestaOfrecida;

	public ApuestaSencilla(double cantidad, Pronostico pronostico,
			String apuestaOfrecida, int usuarioApostador) {
		super(cantidad, pronostico, usuarioApostador);
		this.apuestaOfrecida = apuestaOfrecida;
		this.setTipoApuesta(0);

	}

	public String getApuestaOfrecida() {
		return apuestaOfrecida;
	}

	public void setApuestaOfrecida(String apuestaOfrecida) {
		this.apuestaOfrecida = apuestaOfrecida;
	}
}
